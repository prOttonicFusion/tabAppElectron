{
  "name": "tabapp",
  "version": "0.0.1",
  "description": "A simple bar tab desktop app",
  "scripts": {
    "lint": "eslint -c .eslintrc --ext .js,.ts .",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf ./src/**/*.js ./src/*.js *.js"
  },
  "repository": "https://github.com/prOttonicFusion/tabAppElectron",
  "keywords": [
    "Electron",
    "typescript"
  ],
  "author": "prOttonicFusion",
  "license": "GPL-2.0",
  "build": {
    "appId": "com.tabapp.app",
    "linux": {
      "category": "Utility",
      "icon": "./build/icons/linux"
    },
    "mac": {
      "icon": "build/icons/mac/icon.icns"
    },
    "win": {
      "icon": "build/icons/Icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.11.7",
    "eslint": "^7.15.0",
    "eslint-config-prottonic": "^1.2.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@types/sqlite3": "^3.1.6",
    "sqlite3": "^5.0.0"
  }
}
